{
    "zenMode.hideStatusBar": false,
    "zenMode.hideActivityBar": false,
    "zenMode.hideTabs": false,
    "zenMode.restore": true,
    "explorer.confirmDragAndDrop": false,
    "editor.fontSize": 12,
    "editor.wordWrap": "on",
    "window.zoomLevel": 0,
    "vim.foldfix": true,
    "vim.statusBarColorControl": false,
    "vim.highlightedyank.enable": true,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.currentLine.enabled": false,
    "vim.highlightedyank.duration": 100,
    "vim.hlsearch": true,
    "vim.ignorecase": false,
    "editor.minimap.enabled": false,
    "projectManager.showProjectNameInStatusBar": false,
    "projectManager.removeCurrentProjectFromList": false,
    "vim.easymotion": true,
    "vim.useSystemClipboard": true,
    "vim.sneak": true,
    "vim.handleKeys": {
        "<C-b>": false,
    },
    
    "vim.insertModeKeyBindings": [
        {
            "before": ["<C-l>"],
            "after" : ["b", "o", "b", "<ESC>", "v","i","w","<C-l>", "d", "<UP>", "d", "d", "/", "b", "o", "b", "<CR>", "c", "g", "n"]
        },
    ],
    "vim.insertModeKeyBindingsNonRecursive": [
        {
            "before": [";", "a"],
            "after" : ["<ESC>", "A"]
        },
        {
            "before": [";", ";"],
            "after" : ["<ESC>", "l", "m", "m", "A", ";", "<C-o>", "`", "m"]
        },
        {
            "before"  : [")", ")"],
            "after"   : ["(", "<C-o>", "/", "$", "|", ",", "|", ";", "<CR>", ")"],
            "commands": [":noh"]
        },
        {"before": ["<LEADER>", "{"], "after": ["<DOWN>", "<ESC>", "v", "i", "i", "S", "{", "g", "v", "="]},
        {
            "before": [">", ">"],
            "after" : ["<ESC>", "F", "<", "y", "e", "f", ">", "p", "a", ">", "<C-o>", "T", "<", "/", "<LEFT>", "<LEFT>"]
        },
        {
            "before": [">", "<ENTER>"],
            "after" : ["<ESC>", "y", "y", "p", "e", "l", "d", "t", ">", "F", "<", "a", "/", "<C-o>", "O", " ", " ", " ", " "]
        },
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["<ENTER>"],
            "after": ["%"]
        },
        {
            "before": ["<LEADER>", "/"],
            "commands": [":noh"]
        },
        {
            "before": ["]"],
            "after": ["]", "}"]
        },
        {
            "before": ["["],
            "after": ["[", "{"]
        },
        {
            "before": ["+"],
            "commands": ["scrollLineDown", "scrollLineDown"]
        },
        {
            "before": ["-"],
            "commands": ["scrollLineUp", "scrollLineUp"]
        },
        {
            "before": ["Y"],
            "after" : ["y", "$"]
        },
        {
            "before": ["<LEADER>", "i"],
            "after" : ["B", "i"]
        },
        {
            "before": ["<LEADER>", "a"],
            "after" : ["E", "a"]
        },
        {
            "before": ["<LEADER>", "d"],
            "after" : ["0", "v", "/", "{", "<ENTER>", "%", "V", "d"]
        },
        {
            "before"  : ["<LEADER>", "t"],
            "commands": ["wwm.aligncode"]
        },
        {
            "before": ["<C-c>"],
            "after": ["v","i","w","<C-c>", "b", "b","/","<C-v>","<ENTER>", "c", "g", "n"]
        },
        { "before": ["'", "y"], "after": ["m", "m", "^", "y", "$", "`", "m"] },
        { "before": ["'", "d"], "after": ["^", "D"] },
        { "before": ["'", "c"], "after": ["c", "c"] },
        {
            "before": ["z", "z"],
            "after" : ["z", "z", "3", "<C-e>"]
        },
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": ["<ENTER>"],
            "after" : ["%"]
        },
        {
            "before"  : ["<LEADER>", "t"],
            "commands": ["wwm.aligncode"]
        },
        {
            "before": ["<C-l>"],
            "commands" : ["extension.debugWrapAfter"]
        },
        {
            "before": ["n"],
            "after" : ["y", "/", "<C-r>", "0", "<CR>"]
        },
        {
            "before": ["N"],
            "after" : ["y", "<LEFT>", "/", "<C-r>", "0", "<CR>", "N"]
        },
        {
            "before": [">"],
            "after" : [">", "g", "v"]
        },
        {
            "before": ["<"],
            "after" : ["<", "g", "v"]
        },	
        {
            "before": ["<C-d>"],
            "after" : ["y", "<LEFT>", "/", "<C-r>", "0", "<CR>", "c", "g", "n"]
        },
        {
            "before": ["m"],
            "after" : ["S"]
        },
    ],
    "editor.foldingStrategy": "indentation",
    "workbench.sideBar.location": "left",
    "files.eol": "\n",
    "workbench.editor.enablePreview": false,
    "explorer.confirmDelete": false,
    "emmet.showExpandedAbbreviation": "never",
    "workbench.colorTheme": "Monokai",
    "editor.multiCursorModifier": "ctrlCmd",
    "[dotenv]": {},
    "editor.suggestOnTriggerCharacters": false,
    "[javascript]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "[html]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "javascript.suggest.enabled": false,
    "typescript.suggest.enabled": false,
    "javascript.suggest.completeJSDocs": false,
    "javascript.suggest.names": false,
    "javascript.suggest.paths": false,
    "javascript.suggestionActions.enabled": false,
    "editor.snippetSuggestions": "top",
    "prettier.endOfLine": "cr",
    "javascript.format.enable": false,
}
